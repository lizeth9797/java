import java.util.*;
import java.io.*;

class Solution{
    public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        System.out.println("Q:");
        int t=in.nextInt();
        
        for(int i=0;i<t;i++){
            System.out.print("a:");
            int a = in.nextInt();
            System.out.print("b:");
            int b = in.nextInt();
            System.out.print("n:");
            int n = in.nextInt();
            
            System.out.println(a+b+n);
            //for con la formula
            
        }
        
        in.close();
    }
}