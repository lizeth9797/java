import java.util.*;
import java.io.*;

class Solution{
    public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        System.out.println("dame Q:");

        int t=in.nextInt();
        
        for(int i=0;i<t;i++){
            System.out.println("dentro del for");

            int a = in.nextInt();
            int b = in.nextInt();
            int n = in.nextInt();
            
            System.out.println(a+b+n);
        }
        
        in.close();
    }
}