import java.util.*;
import java.io.*;

class SolutionT{
    public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        System.out.print("Q:");
        int t=in.nextInt();
        
        for(int i=0;i<t;i++){
            System.out.print("a:");//1
            int a = in.nextInt();
            System.out.print("b:");//1
            int b = in.nextInt();
            System.out.print("n:");
            int n = in.nextInt();//2
            
            int potencia=0;
            int resultado;
            
            for (int j=0;j<n;j++) {
            	//potencia=0;
            	resultado=0;
            	for(int k=0;k<=j;k++) {
	            	potencia=(int) (Math.pow(2, k));
	            	System.out.print(potencia+" ");
	            	resultado+=potencia;
	             }	
            	System.out.println("valorResultado: "+resultado);
            	System.out.println("--");
        
            }

            
            
        }
        in.close();
    }
}