import java.util.*;
import java.io.*;

class SolutionT{
    public static void main(String []argh){
        Scanner in = new Scanner(System.in);
        System.out.print("Q:");
        int t=in.nextInt();
        
        for(int i=0;i<t;i++){
            System.out.print("a:");//1
            int a = in.nextInt();
            System.out.print("b:");//1
            int b = in.nextInt();
            System.out.print("n:");
            int n = in.nextInt();//2
            
            int potencia;
            int resultado;
            for(int j=0;j<n;j++) {
            	resultado=(int) (Math.pow(2, j));
            	System.out.println(resultado);
            }
            
            
        }
        in.close();
    }
}